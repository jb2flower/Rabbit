<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Main">
    <android.support.constraint.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/main"
            android:orientation="vertical" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:paddingBottom="20dp" android:id="@+id/constraintLayout5">

        <android.support.constraint.ConstraintLayout
                style="@style/haeder" app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                android:id="@+id/header"
                android:background="@color/alpha">
            <android.support.constraint.ConstraintLayout
                    style="@style/header_btn"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintBottom_toBottomOf="@id/header">
                <ImageButton app:srcCompat="@drawable/header_setting_btn"
                             style="@style/header_setting_btn"
                             android:contentDescription="@string/setting_desc"
                             android:id="@+id/btnHeaderSetting"
                             app:layout_constraintTop_toTopOf="parent"
                             app:layout_constraintEnd_toEndOf="parent"
                             android:layout_marginBottom="2dp"
                             android:background="@color/alpha"
                             android:onClick="goSetting" tools:ignore="OnClick"/>
            </android.support.constraint.ConstraintLayout>
        </android.support.constraint.ConstraintLayout>
        <ImageView
                android:id="@+id/imgProfile"
                android:layout_width="wrap_content"
                android:layout_height="94dp"
                android:layout_gravity="center_horizontal"
                app:srcCompat="@drawable/logo3"
                app:layout_constraintTop_toBottomOf="@+id/header" app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                android:contentDescription="@string/image_desc"/>
        <LinearLayout android:layout_width="match_parent" android:layout_height="wrap_content"
                      android:orientation="vertical" app:layout_constraintTop_toBottomOf="@+id/imgProfile">
            <TextView
                    android:id="@+id/txtName"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="@string/name"
                    android:textColor="@android:color/black"
                    android:textSize="18sp" android:paddingTop="10dp" android:paddingRight="10dp"
                    android:paddingLeft="10dp"/>
        </LinearLayout>
    </android.support.constraint.ConstraintLayout>
    <LinearLayout
            android:layout_width="421dp"
            android:layout_height="363dp"
            android:orientation="vertical" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintTop_toBottomOf="@+id/constraintLayout5">

        <TextView
                android:id="@+id/textView7"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="5dp"
                android:text="내 위치"/>
        <com.google.android.gms.maps.MapView
                android:layout_width="409dp"
                android:layout_height="334dp" android:id="@+id/mapView2" android:layout_gravity="fill"
                android:foregroundGravity="fill"/>

    </LinearLayout>
        <!--<ImageButton-->
                <!--android:layout_width="384dp"-->
                <!--android:layout_height="93dp" app:srcCompat="@drawable/ppiyong123"-->
                <!--android:id="@+id/imageButton2" android:layout_marginLeft="12dp"/>-->
</android.support.constraint.ConstraintLayout>